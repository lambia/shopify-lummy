                {% comment %} ************************************** {% endcomment %}
                {% comment %} *********** LUMMY LINE-ITEM ********** {% endcomment %}
                {% comment %} ************************************** {% endcomment %}

                <table id="priceTable">
                  <thead>
                    <tr>
                      <td class="left_cell">Metri lineari</td>
                      <td class="right_cell">Prezzo al metro</td>
                    </tr>
                  </thead>
