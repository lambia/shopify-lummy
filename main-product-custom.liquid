                        <td class="left_cell">&lt; 3 metri</td>
                        <td class="right_cell">15.00 €/m</td>
                      </tr>
                      <tr>
                        <td class="left_cell">tra 3 e 10 metri</td>
                        <td class="right_cell">13.90 €/m</td>
                      </tr>
                      <tr>
                        <td class="left_cell">tra 10 e 25 metri</td>
                        <td class="right_cell">12.90 €/m</td>
                      </tr>
                      <tr>
                        <td class="left_cell">&gt; 25 metri</td>
                        <td class="right_cell">Richiedi una <a href="/pages/contact">quotazione</a></td>
                      </tr>
                  </tbody>
                </table>
                
                {% comment %} ************************************** {% endcomment %}
